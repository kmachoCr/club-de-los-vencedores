<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lecz.clubdelosvencedores" >

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".register.RegisterActivityOne"
            android:label="@string/title_activity_register_activity_one" >
        </activity>
        <activity
            android:name=".register.RegisterActivityTwo"
            android:label="@string/title_activity_register_activity_two" >
        </activity>
        <activity
            android:name=".register.RegisterActivityThree"
            android:label="@string/title_activity_register_activity_three" >
        </activity>
        <activity
            android:name=".register.RegisterActivityFour"
            android:label="@string/title_activity_register_activity_four" >
        </activity>
        <activity
            android:name=".register.RegisterActivityFive"
            android:label="@string/title_activity_register_activity_five" >
        </activity>
        <activity
            android:name=".general.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".general.BroadcastService" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>
        <service
            android:name=".general.NotificationService"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name" >
        </service>
    </application>

</manifest>
